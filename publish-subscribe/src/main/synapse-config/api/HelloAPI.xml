<?xml version="1.0" encoding="UTF-8"?>
<api context="/hello" name="HelloAPI" xmlns="http://ws.apache.org/ns/synapse">
    <resource methods="GET" url-mapping="/queue">
        <inSequence>
            <payloadFactory media-type="json">
                <format>{
					"firstName": "anupam",
					"lastName": "gogoi"
					}</format>
                <args/>
            </payloadFactory>
            <log level="full"/>
            <send>
                <endpoint key="QueueEP"/>
            </send>
        </inSequence>
        <outSequence>
            <respond/>
        </outSequence>
        <faultSequence/>
    </resource>
</api>
